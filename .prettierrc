{
	"/^[^\\d+]/": "lexical",
	"/^\\d+/": "numeric",
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"overrides": [
		{
			"files": [".prettierrc", ".eslintrc", ".lintstagedrc"],
			"options": { "parser": "json" }
		}
	],
	"placeThisFirst": null,
	"pluginSearchDirs": false,
	"plugins": ["prettier-plugin-tailwindcss"],
	"printWidth": 100,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"tailwindConfig": "./tailwind.config.ts",
	"trailingComma": "es5",
	"useTabs": true
}
